---
import RawLayout from './RawLayout.astro';
import Header from './Header.astro';
import Footer from './Footer.astro';
interface Props {
	title: string;
}
const { title } = Astro.props;
---

<RawLayout title={title}>
	<Header></Header>
		<div class="px-3">
			<slot/>
		</div>
	<Footer></Footer>
</RawLayout>
